# Copy this file to bootstrap_defaults.env and update the values for your environment.
# The real file is ignored by git so secrets (tokens, API keys) stay local.

DEFAULT_RANCHER_URL=https://localhost:8443
DEFAULT_RANCHER_TOKEN=token-xxxx:yyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyy
DEFAULT_RANCHER_CLUSTER=local
# Example context: local:p-abc12 (obtain via `rancher projects ls`)
DEFAULT_RANCHER_CONTEXT=
# Leave blank if backend is disabled and no NGC secret is required.
DEFAULT_NGC_KEY_FILE=
DEFAULT_KUBECTL_BIN=/var/lib/rancher/rke2/bin/kubectl
DEFAULT_RENDER_OUTPUT=/home/hpadmin/HPE-Nvidia-Vista-3D/rancher/vista3d.yaml
DEFAULT_INSTALL_STORAGE=true
DEFAULT_NON_INTERACTIVE=true
DEFAULT_SKIP_TLS_VERIFY=true

